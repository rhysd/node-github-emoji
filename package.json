{
  "name": "github-emoji",
  "version": "1.1.0",
  "description": "GitHub emoji collection",
  "main": "index.js",
  "files": [
    "README.md",
    "LICENSE.txt",
    "images",
    "emoji.json",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "dts": "tsc -p src -d",
    "tsc": "tsc -p src",
    "build": "npm-run-all -p tsc dts",
    "watch": "tsc -p src --watch",
    "update-emoji": "bash ./scripts/update-emoji.sh",
    "update": "npm-run-all update-emoji build test lint",
    "test": "mocha test/*.ts",
    "lint": "tslint -p src",
    "coverage": "nyc npm test",
    "watch-test": "guard",
    "prepare": "npm run build",
    "prepublishOnly": "npm-run-all test lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/node-github-emoji.git"
  },
  "keywords": [
    "GitHub",
    "emoji"
  ],
  "author": "rhysd <https://github.com/rhysd>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/node-github-emoji/issues"
  },
  "homepage": "https://github.com/rhysd/node-github-emoji#readme",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
